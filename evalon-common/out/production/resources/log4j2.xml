<?xml version="1.0" encoding="UTF-8"?>
<configuration status="ERROR">
    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{MM-dd HH:mm:ss.SSS} %-5level%msg%n"/>
        </Console>
    </appenders>
    <loggers>
        <root level="off">
            <appender-ref ref="Console"/>
        </root>
    </loggers>
</configuration>