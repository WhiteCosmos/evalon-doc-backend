<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
    <appid value="me.ele.napos.evalon" />
    <stacktrace value="true"/>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <!-- 对日志进行格式化 -->
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %le %lo[%thread]: %msg</pattern>
        </encoder>
    </appender>
    <root level="ERROR">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>